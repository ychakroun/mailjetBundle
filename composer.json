{
    "name": "mailjet/mailjet-bundle",
    "description": "Symfony bundle for Mailjet API V3",
    "type": "symfony-bundle",
    "homepage": "https://github.com/mailjet/mailjetBundle",
    "license": "MIT",
    "keywords": [
        "Symfony",
        "MailJet",
        "API",
        "V3",
        "User",
        "bundle"
    ],
    "authors": [
        {
            "name": "Titouan BENOIT",
            "email": "titouan.benoit@gmx.fr",
            "homepage": "https://github.com/Nightbr"
        },
        {
            "name": "Mailjet API",
            "email": "api@mailjet.com",
            "homepage": "https://dev.mailjet.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mailjet\\MailjetBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "mailjet/mailjet-apiv3-php": "^1.2",
        "mailjet/mailjet-swiftmailer": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "~4@dev",
        "symfony/symfony": "~4.0 | ~4.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:ychakroun/MailjetSwiftMailer.git"
        }
    ]
}
